@prefix ex: <http://example.org/> .
@prefix shds: <https://w3id.org/shacl-ds#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

ex:shapeGraphNot1 shds:targetGraphCombination [
    shds:minus (ex:dataGraph1 ex:dataGraph3);
].

ex:shapeGraphNot2 shds:targetGraphCombination [
    shds:minus (ex:dataGraph1 ex:dataGraph4);
].


ex:shapeGraphNot1 {
ex:PersonShape
    a sh:NodeShape ;
    sh:targetClass foaf:Person ;    
    sh:property [                 
        sh:path foaf:knows ;
        sh:minCount 1 ;
    ] ;
}

ex:shapeGraphNot2 {
ex:PersonShape
    a sh:NodeShape ;
    sh:targetClass foaf:Person ;    
    sh:property [                 
        sh:path foaf:knows ;
        sh:minCount 1 ;
    ] ;
}